\"CC_Token\"); \r\n\r\n        const role = localStorage.getItem(\"CC_role\");\r\n\r\n\r\n        // If token exists and Socket is NULL\r\n        if (token && !socket) {\r\n\r\n            // Connect to the server and validate the token\r\n            // This will go to the function in the Server where we are \r\n            // Initializing the IO middleware\r\n            // Below passing the token in the local Storage to the Server and validate\r\n            const newSocket = io(\"http://localhost:8000\", {\r\n                query: {\r\n                    token: localStorage.getItem(\"CC_Token\"),\r\n                    role: localStorage.getItem(\"CC_role\")\r\n                },\r\n            });\r\n\r\n\r\n            newSocket.on(\"disconnect\", () => {\r\n\r\n                setSocket(null);\r\n                setTimeout(setupSocket, 3000);\r\n                makeToast(\"error\", \"Socket Disconnected!\");\r\n                setIsAuthenticated(false);\r\n\r\n            });\r\n\r\n\r\n            // This call the IO.ON(\"connection\") function\r\n            // We can attach a listener to fire when we've connected to the server\r\n            newSocket.on(\"connect\", () => {\r\n                  \r\n                makeToast(\"success\", \"Socket Connected!\");\r\n\r\n            });\r\n\r\n            setSocket(newSocket);\r\n\r\n            setIsAuthenticated(true);\r\n\r\n            setRole(role);\r\n                            \r\n        }\r\n\r\n    };\r\n\r\n\r\n    // When the page loads, this gets Executed first\r\n    useEffect(() => {\r\n        setupSocket();\r\n        //eslint-disable-next-line\r\n    }, []);\r\n\r\n\r\n    return (\r\n        <div>\r\n           {/* The variables below will be available to be accessed to all Lower Level components */}\r\n            <AuthContext.Provider value={{ userId, setUserId, isAuthenticated, setIsAuthenticated, role, setRole, setupSocket, socket}}>\r\n                { children }\r\n            </AuthContext.Provider>\r\n        </div>\r\n    )\r\n\r\n}\r\n\r\n\r\n","C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\Footer.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Services\\RoutesServices.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\NavBar\\NavBar.js",["108"],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Toaster.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\LoginPage.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\NavBar\\LeftNavContent.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\NavBar\\RightNavContent.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\Chat\\ChatPage.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\hoc\\UnPrivateRoute.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\hoc\\PrivateRoute.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\Chat\\CreateUserPage.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\CreateUser\\RegisterPage.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\Home\\MainBody.js",["109"],"import React from \"react\";\r\nimport Questions from \"./Questions\";\r\nimport Body from \"./IndexBody\";\r\nimport LivePrograms from \"./LivePrograms\";\r\nimport Header from \"./IndexHeader