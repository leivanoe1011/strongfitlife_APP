\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\MyStory\\index.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Services\\AuthService.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\Home\\Questions.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\Home\\IndexHeader.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\Home\\BlogUpdates.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\Chat\\CreateChatPage.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\Home\\IndexBody.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Services\\ServerService.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\Home\\Testimonials.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\Home\\LivePrograms.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\Home\\ContactForm.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\MyStory\\Header.js",["138","139"],"import React from \"react\";\r\nimport Container from \"react-bootstrap/Container\";\r\nimport Row from \"react-bootstrap/Row\";\r\nimport Col from \"react-bootstrap/Col\";\r\nimport vanessa from \"../../images/vanessa-flores.jpeg\";\r\n\r\nfunction Header() {\r\n  return (\r\n    <div>\r\n      <Container>\r\n        <Row className=\"justify-content-center mt-5\">\r\n          <Col sm={12} md={6} lg={6}>\r\n            <img\r\n              className=\"float-start myStoryPhoto rounded img-fluid\"\r\n              src={vanessa}\r\n            ></img>\r\n          </Col>\r\n          <Col sm={12} md={6} lg={6}>\r\n            <div className=\"myStoryText\">\r\n              <h4>\r\n                Do you struggle through boring workouts day after day, feeling\r\n                unmotivated and never seeing the results you long for?\r\n              </h4>\r\n              <p>\r\n                Not so long ago I was stuck in the same cycle, trying\r\n                desperately to find a healthy balance between food, exercise and\r\n                mental wellbeing.\r\n              </p>\r\n              <p>\r\n                My story begins in college where I started working out to escape\r\n                the pressures of school. While that was a good thing, I took it\r\n                waaay too far. I became obsessed with being skinny and pushed\r\n                myself to workout two hours every day while barely eating\r\n                anything.\r\n              </p>\r\n              <p>\r\n                It’s no surprise these extreme measures caused my health to\r\n                decline, but instead of finding the right balance, I reverted to\r\n                the other extreme: binge eating. I put on a lot of weight and\r\n                went up 6 sizes.\r\n              </p>\r\n              <p>\r\n                My flip flopping took its toll. I was depressed, none of my\r\n                clothes fit, and I hated life because I was constantly obsessing\r\n                over food — I was stuck in a cycle of stuffing my face and then\r\n                feeling guilty afterward.\r\n              </p>\r\n              <p>\r\n                Sure, I tried to change. I joined different types of gyms and\r\n                tried various styles of eating, but this just made me more\r\n                frustrated because I wasn’t seeing results. I was going through\r\n                the motions instead of pushing myself to the point where I could\r\n                experience real transformation.\r\n              </p>\r\n              <h4 className=\"myStoryTitle\">Then came the wake-up call.</h4>\r\n              <p>\r\n                One day it hit me — I realized my struggle and depression was\r\n                beginning to seriously affect my relationships with my husband,\r\n                family and friends.\r\n              </p>\r\n              <p>\r\n                I didn’t want to lose them, so I made the decision to invest in\r\n                myself and explore different options that would work for me.\r\n              </p>\r\n              <p>\r\n                I landed in a new gym where I learned proper form and technique.\r\n                The workouts were challenging, and I loved the sore feeling I\r\n                felt after working out hard and smart! During this time, I\r\n                started making simple food changes and learning how to fuel my\r\n                body.\r\n              </p>\r\n              <h4 className=\"myStoryTitle\">\r\n                Even better? I lost over 30 pounds in the process!\r\n              </h4>\r\n              <p>\r\n                After experiencing such incredible results, I knew I wanted to\r\n                help others make the same changes. I’m now a Beastmode fitness &\r\n                health coach who’s passionate about teaching others how to feel\r\n                like a badass in their bodies.\r\n              </p>\r\n              <p>\r\n                Becoming fit doesn't have to be time consuming, complicated, and\r\n                expensive. I’ve created my signature Strong Fit Program, Unleash\r\n                the Beast, to help you break through fitness plateaus and gain\r\n                strength and confidence from the convenience and safety of your\r\n                home.\r\n              </p>\r\n            </div>\r\n          </Col>\r\n        </Row>\r\n        <Row className=\"text-center\">\r\n          <Col sm={12} md={12} lg={12}>\r\n            <a\r\n              className=\"btn btn-large text-light mt-5\"\r\n              style={{ backgroundColor: \"salmon\" }}\r\n            >\r\n              READY TO EXPERIENCE REAL RESULTS? START HERE\r\n            </a>\r\n          </Col>\r\n        </Row>\r\n      </Container>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Header;\r\n","C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Components\\MyStory\\Body.js",[],"C:\\Users\\noe.leiva\\Documents\\OtherProjects\\strongfitlife_APP\\strongfitlife_APP\\client\\src\\Services\\History.js",[],{"ruleId":"140","replacedBy":"141"},{"ruleId":"142","replacedBy":"143"},{"ruleId":"144","severity":1,"message":"145","line":9,"column":1,"nodeType":"146","endLine":106,"endColumn":2},{"ruleId":"147","severity":1,"message":"148","line":63,"column":13,"nodeType":"149","endLine":63,"endColumn":125},{"ruleId":"150","severity":1,"message":"151","line":35,"column":37,"nodeType":"152","messageId":"153","endLine":35,"endColumn":43},{"ruleId":"150","severity":1,"message":"154","line":9,"column":10,"nodeType":"152","messageId":"153","endLine":9,"endColumn":21},{"ruleId":"147","severity":1,"message":"148","line":13,"column":13,"nodeType":"149","endLine":16,"endColumn":14},{"ruleId":"155","severity":1,"message":"156","line":93,"column":13,"nodeType":"149","endLine":96,"endColumn":14},"no-native-reassign",["157"],"no-negated-in-lhs",["158"],"import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-unused-vars","'userId' is assigned a value but never used.","Identifier","unusedVar","'AuthContext' is defined but never used.","js